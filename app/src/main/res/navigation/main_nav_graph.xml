<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/man_nav_graph"
    app:startDestination="@id/enabledNotesListFragment">

    <fragment
        android:id="@+id/enabledNotesListFragment"
        android:name="com.trippntechnology.tnt.flashcards.ux.fragments.main.EnabledNotesListFragment"
        android:label="EnabledNotesListFragment"
        tools:layout="@layout/fragment_enabled_notes_list_view">
        <action
            android:id="@+id/action_enabledNotesListFragment_to_flashCardFragment"
            app:destination="@id/flashCardFragment" />
        <action
            android:id="@+id/action_enabledNotesListFragment_to_selectNoteFragment"
            app:destination="@id/selectNoteFragment" />
    </fragment>

    <fragment
        android:id="@+id/flashCardFragment"
        android:name="com.trippntechnology.tnt.flashcards.ux.fragments.main.FlashCardFragment"
        android:label="FlashCardFragment"
        tools:layout="@layout/fragment_flash_card" />
    <fragment
        android:id="@+id/selectNoteFragment"
        android:name="com.trippntechnology.tnt.flashcards.ux.fragments.main.SelectNoteFragment"
        android:label="SelectNoteFragment"
        tools:layout="@layout/fragment_select_note">
        <action
            android:id="@+id/action_selectNoteFragment_to_enabledNotesListFragment2"
            app:destination="@id/enabledNotesListFragment"/>
    </fragment>

</navigation>